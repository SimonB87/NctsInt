    </ActionBlock>
    <ActionBlock>
      <Name value="sendFBIncidentDataRequest" />
      <ActionLangBody IsValid="True">
        <Script><![CDATA[//TODO - dosadit vybraný CO
var selectedCo = "ME123456"
apiobjects.CIC.setHeader("X-CustomsOffice", selectedCo)

var response = apiobjects.CIC.postAsync("FBIncidentDataRequest", form.model.toJson(true))

if response.isError() then
	system.console("There is an error in the response on FBIncidentDataRequest message!")
	system.console(response.getErrorMessage())
	system.console(response.getMessageType())
else
	var responseMessageType = response.getMessageType()
	
	if isTrue(responseMessageType = "BFDataResponse") then
		var inputDataJSON = response.extractJson("TransitOperation")
		navigation.main.IncidentRegistration.withInputDataJSON(inputDataJSON).go()
	else
		if isTrue(responseMessageType = "BFIncidentNoData") then
			var rejection = response.extractJson("TransitOperation")
			form.responseNoData.fromJson(rejection)
			form._rejectionFieldsVisible = true
		endif
	endif
endif]]></Script>
      </ActionLangBody>
      <DataContext>
        <ActionDataContext />
      </DataContext>
    </ActionBlock>
    <ActionBlock>
      <Name value="handler_btn_Send_OnClick" />
      <IsPrivate value="True" />
      <ActionLangBody IsValid="True">
        <Script><![CDATA[globals.busyIndicatorIsRunning = true

if form.model.TransitOperation.MRN.hasValue() then
	form.actions.sendFBIncidentDataRequest()
endif

globals.busyIndicatorIsRunning = false]]></Script>
      </ActionLangBody>
      <DataContext>
        <ActionDataContext />
      </DataContext>
    </ActionBlock>
    <ActionBlock>
      <Name value="init" />
      <ActionLangBody IsValid="True">
        <Script><![CDATA[globals.busyIndicatorIsRunning = true

form.actions.setUrlDeepLink()
//
form.model.TransitOperation.MRN = "20ME123456C4VGPAJ1"

form.apiobjects.AarRejectionReason.startLoadingData(getDateTime())
form._rejectionFieldsVisible = false
globals.busyIndicatorIsRunning = false]]></Script>
      </ActionLangBody>
      <DataContext>
        <ActionDataContext />
      </DataContext>
    </ActionBlock>
    <ActionBlock>
