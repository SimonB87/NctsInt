  <Actions>
    <ActionBlock>
      <Name value="init" />
      <ActionLangBody IsValid="True">
        <Script><![CDATA[globals.busyIndicatorIsRunning = true

if globals.loginResponse.isEmpty() then
	actions.authentication()
endIf

var goToHomeFormResult = form.actions.tryLoadingData()

form.actions.setUrlDeepLink()

form.actions.setBreadCrumbs()

form.actions.setUserModeInGui()

// Need response from server
globals.busyIndicatorIsRunning = false

if goToHomeFormResult then
	form.actions.clearUrlGoToHomeForm()
endIf]]></Script>
      </ActionLangBody>
      <DataContext>
        <ActionDataContext />
      </DataContext>
    </ActionBlock>
    <ActionBlock>
      <Name value="setUrlDeepLink" />
      <ActionLangBody IsValid="True">
        <Script><![CDATA[//TODO - v případě, kdy v URL není mode, tak navigovat uživatele na začátek aplikace
var url = apiobjects.urlRouter.findFormUrl(navigation.main.ConsignmentList.fullName)

if (form.mode.hasValue()) then
	url.addString("mode", form.mode.enumValue())
endif

if (form.GUID.hasValue()) then
	url.addString("GUID", form.GUID)
endif

if (form.sequenceNumber.hasValue()) then
	url.addInt("sequenceNumber", form.sequenceNumber)
endif

url.updatePageUrl()]]></Script>
      </ActionLangBody>
      <DataContext>
        <ActionDataContext />
      </DataContext>
    </ActionBlock>
    <ActionBlock>
      <Name value="handler_GoodsItem_Table_OnRowActivate" />
      <IsPrivate value="True" />
      <ActionLangBody IsValid="True">
        <Script><![CDATA[if form.GUID.hasValue() then
	navigation.main.ConsignmentItem.withGuid(form.GUID).withGoodsItemNumber(params.data.goodsItemNumber).go(form.mode, form.sequenceNumber)
else
	navigation.main.ConsignmentItem.withGoodsItemNumber(params.data.goodsItemNumber).go(form.mode, form.sequenceNumber)
endif]]></Script>
      </ActionLangBody>
      <DataContext>
        <ActionDataContext />
      </DataContext>
    </ActionBlock>
    <ActionBlock>
      <Name value="setBreadCrumbs" />
      <ActionLangBody IsValid="True">
        <Script><![CDATA[apiobjects.breadcrumbsCtrl.init("main::_breadcrumbsCtrlStruct")

apiobjects.breadcrumbsCtrl.removeAll()

apiobjects.breadcrumbsCtrl.addBreadcrumb("houseConsignments", "House Consignments", defaultString(), defaultString(), true)

var houseConsignItemNum = "House Consignment #" + toString(form.sequenceNumber)
apiobjects.breadcrumbsCtrl.addBreadcrumb("houseConsignmentItem", houseConsignItemNum, "#houseConsignmentItem", defaultString(), true)

apiobjects.breadcrumbsCtrl.addBreadcrumb("consignmentItems", "Consignment Items", defaultString(), defaultString(), defaultBool())
]]></Script>
      </ActionLangBody>
      <DataContext>
        <ActionDataContext />
      </DataContext>
    </ActionBlock>
    <ActionBlock>
      <Name value="handler_breadcrumbs1_OnItemClick" />
      <IsPrivate value="True" />
      <ActionLangBody IsValid="True">
        <Script><![CDATA[actions.consignementNavigationBreadcrumbs(params.id, form.mode, form.sequenceNumber, defaultInt(), form.GUID)]]></Script>
      </ActionLangBody>
      <DataContext>
        <ActionDataContext />
      </DataContext>
    </ActionBlock>
    <ActionBlock>
      <Name value="handler_delete_btn_OnClick" />
      <IsPrivate value="True" />
      <ActionLangBody IsValid="True">
        <Script><![CDATA[globals.busyIndicatorIsRunning = true

var confirmFileDelete = apiobjects.pageApi.confirm("Do you want to delete item n. " + toString(params.data.goodsItemNumber) )


if confirmFileDelete = True then
	apiobjects.transitDeclaration.removeConsignmentItem(form.sequenceNumber, params.data.goodsItemNumber)

	form.ConsignmentCollection.clear()
	// TO DO - refresh HouseConsignementList data
	// doda load stejný na initu pro load dat

	var rowsJson = apiobjects.transitDeclaration.getConsignmentItemList(form.sequenceNumber)

	// kontrolovat jestli rowsJson není prázdný, jestli ano, tak přenést uživatele na začátek aplikace

	form.ConsignmentCollection.fromJson(rowsJson)
endIf

// Need response from server
globals.busyIndicatorIsRunning = false]]></Script>
      </ActionLangBody>
      <DataContext>
        <ActionDataContext />
      </DataContext>
    </ActionBlock>
    <ActionBlock>
      <Name value="addConsigItem" />
      <ActionLangBody IsValid="True">
        <Script><![CDATA[// TODO - POSTUP PRÁCE

// 1 navigace na detail

// 2 ale zde v tuto chvíli ještě nebude sequence number

// 3 uživatel vyplní infromace a pak ULOŽÍ PŘES funkci : apiobjects.transitDeclaration.appendNewConsignmentItem()
// V PŘÍPADĚ EDTACE EXISTUJÍCÍHO SE PRACUJE PŘES apiobjects.transitDeclaration.setConsignmentItem()


//navigation to the newly added item
var newItemGoodsItemNumber = form.ConsignmentCollection.count() + 1

apiobjects.transitDeclaration.appendNewConsignmentItem(form.sequenceNumber, "{}")

if form.GUID.hasValue() then
	navigation.main.ConsignmentItem.withGuid(form.GUID).withGoodsItemNumber(newItemGoodsItemNumber).go(form.mode, form.sequenceNumber)
else
	navigation.main.ConsignmentItem.withGoodsItemNumber(newItemGoodsItemNumber).go(form.mode, form.sequenceNumber)
endif]]></Script>
      </ActionLangBody>
      <DataContext>
        <ActionDataContext />
      </DataContext>
    </ActionBlock>
