if form.GUID = mockGuid then
	apiobjects.transitDeclaration.fromMock() // navigation still relies on mock DATA!!!

	var sequenceNumberIsValidInTransitDeclaration = form.actions.doesSequenceNumberExist()

	if form.sequenceNumber.hasValue() and sequenceNumberIsValidInTransitDeclaration then

