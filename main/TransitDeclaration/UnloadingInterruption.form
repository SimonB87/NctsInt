    <ActionBlock>
      <Name value="init" />
      <ActionLangBody IsValid="True">
        <Script><![CDATA[form.guidParent.GUID = form.GUID
var requestJSON = form.guidParent.toJson()

var response = apiobjects.CIC.postAsync("FBDataRequest", requestJSON)

if response.isError() then
	
	var inputError = response.extractJson("Error")
	form.actions.processError(inputError)
	
else

	form.model.fromJson(response.getJsonString())
	
endif]]></Script>
      </ActionLangBody>
      <DataContext>
        <ActionDataContext />
      </DataContext>
    </ActionBlock>
    <ActionBlock>
      <Name value="handler_btn_continue_OnClick" />
      <IsPrivate value="True" />
      <ActionLangBody IsValid="True">
        <Script><![CDATA[//if form.messageToBeSent.Decision.hasValue() then
if form.messageToBeSent.validationErrorsCount() = 0 then
	
	form.messageToBeSent.TransitOperation.MRN = form.model.TransitOperation.MRN
	var request = form.messageToBeSent.toJson()
	var response = apiobjects.CIC.postAsync("FBResolutionToContinue", request)
	
	if response.isError() then
		
		var inputError = response.extractJson("Error")
		form.actions.processError(inputError)
		
	else
		//TODO: kam mam jit?
		navigation.main.DecList.go()
	endif
	
else
	
	globals.globalUiState.isValidationsVisible = true
	
endif]]></Script>
      </ActionLangBody>
      <DataContext>
        <ActionDataContext />
      </DataContext>
    </ActionBlock>
    <ActionBlock>
      <Name value="handler_btn_control_OnClick" />
      <IsPrivate value="True" />
      <ActionLangBody IsValid="True">
        <Script><![CDATA[if form.messageToBeSent.Decision.hasValue() then
	
	form.messageToBeSent.TransitOperation.MRN = form.model.TransitOperation.MRN
	var request = form.messageToBeSent.toJson()
	var response = apiobjects.CIC.postAsync("FBResolutionToControl", request)
	
	if response.isError() then

		var inputError = response.extractJson("Error")
		form.actions.processError(inputError)

	else
		//TODO: kam mam jit?
		navigation.main.DecList.go()
	endif
else
	
	globals.globalUiState.isValidationsVisible = true
	
endif]]></Script>
      </ActionLangBody>
      <DataContext>
        <ActionDataContext />
      </DataContext>
    </ActionBlock>
    <ActionBlock>
      <Name value="processError" />
      <ActionLangBody IsValid="True">
        <Script><![CDATA[//clear global error structures
globals.Errors.serverValidationComplex.clear()
globals.Errors.serverValidationErrors.clear()
//fill global error structures
globals.Errors.serverValidationComplex.fromJson(inputError)
globals.Errors.serverValidationErrors.fromJson(globals.Errors.serverValidationComplex.MessageCheckError.toJson())

globals.globalUiState.isValidationsVisible = true
	
// Inform user about errors
var errorMessage = defaultString()

if (globals.Errors.serverValidationErrors.count() > 1) then 
	errorMessage = "There are " + toString(globals.Errors.serverValidationErrors.count()) + " errors in the message, please fix them to continue."
else
	errorMessage = "There is " + toString(globals.Errors.serverValidationErrors.count()) + " error in the message, please fix it to continue."
endif	
	
apiobjects.notificationsCtrl.addError(errorMessage)]]></Script>
      </ActionLangBody>
