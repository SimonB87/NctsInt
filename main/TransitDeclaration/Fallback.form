          <Panel.Height value="#" />
          <Panel.Width value="*" />
          <Children>
            <Panel>
              <Name value="goBack_pnl" />
              <Orientation value="Horizontal" />
              <Panel.Height value="#" />
              <Panel.Width value="*" />
              <Children>
                <Button>
                  <Name value="goBack_btn" />
                  <Caption value="Go back" />
                  <Panel.Height value="#" />
                  <Panel.Width value="150px" />
