              </CssClass>
            </Panel>
          </Children>
          <CssClass>
            <CssClassConditional>
              <ClassName value="{theme}-panel" />
              <IsThemedStyle value="True" />
            </CssClassConditional>
            <CssClassConditional>
              <ClassName value="absoluteCenter" />
            </CssClassConditional>
          </CssClass>
        </Panel>
      </Children>
      <CssClass>
        <CssClassConditional>
          <ClassName value="{theme}-panel" />
          <IsThemedStyle value="True" />
        </CssClassConditional>
        <CssClassConditional>
          <ClassName value="absoluteCenter" />
          <Condition value="False" />
        </CssClassConditional>
      </CssClass>
    </Panel>
  </Content>
  <DataContext>
    <DataContext>
      <Children>
        <Complex>
          <Name value="model" />
          <Caption value="Model" />
          <Children>
            <Complex>
              <Name value="TransitOperation" />
              <Caption value="Transit operation" />
              <Children>
                <Attribute>
                  <Name value="MRN" />
                  <Caption value="MRN" />
                  <DeclaredType>
                    <AttributeType />
                  </DeclaredType>
                </Attribute>
              </Children>
            </Complex>
            <Attribute>
              <Name value="GUID" />
              <Caption value="GUID" />
              <DeclaredType>
                <AttributeType />
              </DeclaredType>
            </Attribute>
          </Children>
        </Complex>
        <Complex>
          <Name value="dataResponse" />
          <Caption value="Data response" />
        </Complex>
        <Attribute>
          <Name value="_afterSend" />
          <Caption value="_ after send" />
          <DeclaredType>
            <AttributeType>
              <BaseType value="Bool" />
            </AttributeType>
          </DeclaredType>
        </Attribute>
      </Children>
    </DataContext>
  </DataContext>
  <OnCreate>
    <ActionEvent ReferencedAction="~handler_Fallback_OnCreate" />
  </OnCreate>
  <Actions>
    <ActionBlock>
      <Name value="handler_goBack_btn_OnClick" />
      <IsPrivate value="True" />
      <ActionLangBody IsValid="True">
        <Script><![CDATA[navigation.main.DecList.go()]]></Script>
      </ActionLangBody>
      <DataContext>
        <ActionDataContext />
      </DataContext>
    </ActionBlock>
    <ActionBlock>
      <Name value="handler_btn_Send_OnClick" />
      <IsPrivate value="True" />
      <ActionLangBody IsValid="True">
        <Script><![CDATA[globals.busyIndicatorIsRunning = true

if form.model.TransitOperation.MRN.isEmpty() then
	system.console("Nejaka chyba - nevim jeste")
else
	form.actions.sendFBFallbackDataRequest()
endif

globals.busyIndicatorIsRunning = false]]></Script>
      </ActionLangBody>
      <DataContext>
        <ActionDataContext />
      </DataContext>
    </ActionBlock>
    <ActionBlock>
      <Name value="handler_btn_Clear_OnClick" />
      <IsPrivate value="True" />
      <ActionLangBody IsValid="True">
        <Script><![CDATA[form.model.clear()
form._afterSend = false]]></Script>
      </ActionLangBody>
      <DataContext>
        <ActionDataContext />
      </DataContext>
    </ActionBlock>
    <ActionBlock>
      <Name value="sendFBFallbackDataRequest" />
      <ActionLangBody IsValid="True">
        <Script><![CDATA[var response = apiobjects.CIC.postAsync("FBFallbackDataRequest", form.model.toJson())

if response.isError() then
	system.console("There is an error in the response on FBFallbackDataRequest message!")
	form._afterSend = true
else
	var inputDataJson = response.getJsonString() 

	if isTrue(response.getMessageType()="BFFallbackData") then
		system.console(inputDataJson)
		form.model.fromJson(response.getJsonString())
		navigation.main.Header.withGUID(form.model.GUID).withInputDataJSON(inputDataJson).go(enums.declarationFormMode.fallback)

	else
		if isTrue(response.getMessageType()="BFFallbackNoData") then
			form._afterSend = true
			system.console("No data")
		else
			//tady kdyz by nahodou prisla jeste uplne jina zprava
		endif
	endif	
endif]]></Script>
      </ActionLangBody>
      <DataContext>
        <ActionDataContext />
      </DataContext>
    </ActionBlock>
    <ActionBlock>
      <Name value="handler_Fallback_OnCreate" />
      <IsPrivate value="True" />
      <ActionLangBody IsValid="True">
        <Script><![CDATA[form._afterSend = false]]></Script>
      </ActionLangBody>
      <DataContext>
        <ActionDataContext />
      </DataContext>
    </ActionBlock>
  </Actions>
</Form>