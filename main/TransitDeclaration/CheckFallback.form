    <ActionBlock>
      <Name value="handler_btn_Clear_OnClick" />
      <IsPrivate value="True" />
      <ActionLangBody IsValid="True">
        <Script><![CDATA[form.model.clear()
form.isValidationSummaryVisible = false
form.model.setTouched(false)
form._afterSend = false]]></Script>
      </ActionLangBody>
      <DataContext>
        <ActionDataContext />
      </DataContext>
    </ActionBlock>
    <ActionBlock>
      <Name value="sendFBCheckFallbackMRN" />
      <ActionLangBody IsValid="True">
        <Script><![CDATA[var response = apiobjects.CIC.postAsync("FBCheckFallbackMRN", form.model.toJson(true))

if response.isError() then
	form.errorMessage = localized.ThereIsAnErrorInTheResponseFBCheckFallbackMRN
	form._afterSend = true
else
	form.model.fromJson(response.getJsonString())
	
	var messageType = response.getMessageType()
	
	if isTrue(messageType = "BFCheckFallbackMRNResponse") then
		if form.model.FallbackDestination.newMRN.hasValue() then
			navigation.main.FallbackRegistration.withInputOriginalDocument(form.model.FallbackDestination.originalDocument).withInputOriginalDocumentState(form.model.FallbackDestination.originalDocumentState.Description).go(form.model.FallbackDestination.newMRN)
		else
			form.errorMessage = form.model.FallbackDestination.originalDocumentState.Description
			form._afterSend = true
		endif	
	else
		form.errorMessage = localized.ServerSentUnknownMessage
		form._afterSend = true
	endif
endif]]></Script>
      </ActionLangBody>
      <DataContext>
        <ActionDataContext />
      </DataContext>
    </ActionBlock>
    <ActionBlock>
      <Name value="handler_Fallback_OnCreate" />
      <IsPrivate value="True" />
      <ActionLangBody IsValid="True">
        <Script><![CDATA[actions.userAccessViaDeeplink()

form.actions.setUrlDeepLink()

form._afterSend = false

if form.inputOriginalDocument.hasValue() then
	form.model.FallbackDestination.originalDocument = form.inputOriginalDocument
	form.actions.sendCheckFallback()
endif]]></Script>
      </ActionLangBody>
      <DataContext>
        <ActionDataContext />
      </DataContext>
    </ActionBlock>
    <ActionBlock>
      <Name value="setUrlDeepLink" />
      <ActionLangBody IsValid="True">
        <Script><![CDATA[var url = apiobjects.urlRouter.findFormUrl(navigation.main.CheckFallback.fullName)

url.updatePageUrl()]]></Script>
      </ActionLangBody>
      <DataContext>
        <ActionDataContext />
      </DataContext>
    </ActionBlock>
    <ActionBlock>
      <Name value="sendCheckFallback" />
      <ActionLangBody IsValid="True">
        <Script><![CDATA[globals.busyIndicatorIsRunning = true

if form.model.FallbackDestination.isValid() then
	form.actions.sendFBCheckFallbackMRN()
else
	form.isValidationSummaryVisible = true
	form.errorMessage = localized.OriginalDocumentMustHaveAValue
	form._afterSend = true	
endif

globals.busyIndicatorIsRunning = false]]></Script>
      </ActionLangBody>
      <DataContext>
        <ActionDataContext />
      </DataContext>
    </ActionBlock>
