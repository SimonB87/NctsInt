  <OnCreate>
    <ActionEvent ReferencedAction="~handler_Fallback_OnCreate" />
  </OnCreate>
  <Actions>
    <ActionBlock>
      <Name value="handler_goBack_btn_OnClick" />
      <IsPrivate value="True" />
      <ActionLangBody IsValid="True">
        <Script><![CDATA[navigation.main.DecList.go()]]></Script>
      </ActionLangBody>
      <DataContext>
        <ActionDataContext />
      </DataContext>
    </ActionBlock>
    <ActionBlock>
      <Name value="handler_btn_Send_OnClick" />
      <IsPrivate value="True" />
      <ActionLangBody IsValid="True">
        <Script><![CDATA[globals.busyIndicatorIsRunning = true

if form.model.FallbackDestination.originalDocument.isEmpty() then
	form.errorMessage = "Original document must have a value"
	form._afterSend = true
else
	form.actions.sendFBCheckFallbackMRN()
endif

globals.busyIndicatorIsRunning = false]]></Script>
      </ActionLangBody>
      <DataContext>
        <ActionDataContext />
      </DataContext>
    </ActionBlock>
    <ActionBlock>
      <Name value="handler_btn_Clear_OnClick" />
      <IsPrivate value="True" />
      <ActionLangBody IsValid="True">
        <Script><![CDATA[form.model.clear()
form._afterSend = false]]></Script>
      </ActionLangBody>
      <DataContext>
        <ActionDataContext />
      </DataContext>
    </ActionBlock>
    <ActionBlock>
      <Name value="sendFBCheckFallbackMRN" />
      <ActionLangBody IsValid="True">
        <Script><![CDATA[var response = apiobjects.CIC.postAsync("FBCheckFallbackMRN", form.model.toJson(true))

if response.isError() then
	form.errorMessage = "There is an error in the response on FBCheckFallbackMRN message!"
	form._afterSend = true
else
	form.model.fromJson(response.getJsonString())
	if isTrue(response.getMessageType()="BFCheckFallbackMRNResponse") then
		if form.model.FallbackDestination.newMRN.hasValue() then
			navigation.main.FallbackRegistration.withInputOriginalDocument(form.model.FallbackDestination.originalDocument).go(form.model.FallbackDestination.newMRN)
		else
			form.errorMessage = form.model.FallbackDestination.originalDocumentState.Description
			form._afterSend = true
		endif	
	else
		form.errorMessage = "Server sent unknown message"
		form._afterSend = true
	endif
endif]]></Script>
      </ActionLangBody>
      <DataContext>
        <ActionDataContext />
      </DataContext>
    </ActionBlock>
    <ActionBlock>
      <Name value="handler_Fallback_OnCreate" />
      <IsPrivate value="True" />
      <ActionLangBody IsValid="True">
        <Script><![CDATA[form.actions.setUrlDeepLink()

form._afterSend = false]]></Script>
      </ActionLangBody>
      <DataContext>
        <ActionDataContext />
      </DataContext>
    </ActionBlock>
    <ActionBlock>
      <Name value="setUrlDeepLink" />
      <ActionLangBody IsValid="True">
        <Script><![CDATA[var url = apiobjects.urlRouter.findFormUrl(navigation.main.CheckFallback.fullName)

url.updatePageUrl()]]></Script>
      </ActionLangBody>
      <DataContext>
        <ActionDataContext />
      </DataContext>
    </ActionBlock>
  </Actions>
</Form>