  <DataContext>
    <DataContext>
      <Children>
        <Complex>
          <Name value="model" />
          <Caption value="Model" />
          <Children>
            <Complex>
              <Name value="FallbackDestination" />
              <Caption value="Fallback destination" />
              <Children>
                <Attribute>
                  <Name value="originalDocument" />
                  <Caption value="Original document" />
                  <IsRequired value="True" />
                  <DeclaredType>
                    <AttributeType />
                  </DeclaredType>
                </Attribute>
                <Reference>
                  <Name value="originalDocumentState" />
                  <Caption value="Original document state" />
                  <Children>
                    <Attribute>
                      <Name value="Key" />
                      <Caption value="Key" />
                      <DeclaredType>
                        <AttributeType>
                          <BaseType value="Integer" />
                        </AttributeType>
                      </DeclaredType>
                    </Attribute>
                    <Attribute>
                      <Name value="Description" />
                      <Caption value="Description" />
                      <DeclaredType>
                        <AttributeType />
                      </DeclaredType>
                    </Attribute>
                  </Children>
                </Reference>
                <Attribute>
                  <Name value="newMRN" />
                  <Caption value="New MRN" />
                  <DeclaredType>
                    <AttributeType />
                  </DeclaredType>
                </Attribute>
              </Children>
            </Complex>
          </Children>
        </Complex>
