    <ActionBlock>
      <Name value="handler_button2_OnClick" />
      <IsPrivate value="True" />
      <ActionLangBody IsValid="True">
        <Script><![CDATA[var clientValidationErrorsCount = form.Exit.validationErrorsCount()

if clientValidationErrorsCount = 0 then
	form.isLoading = true
	var messageType = "FBExit"
	var requestJsonString = form.Exit.toJson(true)
	
	var Errors = actions.sendMessage(messageType, requestJsonString)

	form.isLoading = false
	
	if isFalse(Errors) then	
		
		navigation.close("OK")
	
	else
	
		form.isValidationSummaryVisible = true
		
	endif
else

	form.Exit.setTouched(TRUE)
	
endif]]></Script>
      </ActionLangBody>
      <DataContext>
        <ActionDataContext />
      </DataContext>
    </ActionBlock>
