        <Script><![CDATA[var country = params.data.country.Code

if(hasValue(country)) then
	var countryWithoutZip = CountryWithoutZip_CL505.getItemByStringColumnAsync("Code", country, defaultDate(), true)

	if(countryWithoutZip.isFound()) then
		form.validationVariables.isPostcodeRequired = false
	else
		form.validationVariables.isPostcodeRequired = true
	endif
else
	form.validationVariables.isPostcodeRequired = false
endif

]]></Script>
