    <ActionBlock>
      <Name value="handler_modifier_3C2E4FDC_qualifierOfIdentification_OnUserChange" />
      <IsPrivate value="True" />
      <ActionLangBody IsValid="True">
        <Script><![CDATA[if isTrue(params.data.qualifierOfIdentification.Code = "W") then
	
	//zhasnu adresu a UNLocode, ale pokud v nich mam nejaka data, tak se zeptam, jestli je muzu smazat
	if (params.data.Address.hasValue() OR params.data.UNLocode.hasValue()) then
	
		if system.confirm("This change can delete some data, do you want to continue?") then
			
			params.data.Address.clear()
			params.data.UNLocode.clear()
			
			//rozsvitim jen GNSS
			form.validationVariables.isLocationUNLocodeVisible = false
			form.validationVariables.isLocationGNSSVisible = true
			form.validationVariables.isLocationAddressVisible = false
			
		endif
	
	else
	
		//rozsvitim jen GNSS
		form.validationVariables.isLocationUNLocodeVisible = false
		form.validationVariables.isLocationGNSSVisible = true
		form.validationVariables.isLocationAddressVisible = false
		
	endif
	
else
	if isTrue(params.data.qualifierOfIdentification.Code = "U") then
	
		//zhasnu adresu a GNSS, ale pokud v nich mam nejaka data, tak se zeptam, jestli je muzu smazat
		if (params.data.GNSS.hasValue() OR params.data.Address.hasValue()) then
		
			if system.confirm("This change can delete some data, do you want to continue?") then
			
				params.data.GNSS.clear()
				params.data.Address.clear()
				
				//rozsvitim jen UNLocode
				form.validationVariables.isLocationUNLocodeVisible = true
				form.validationVariables.isLocationGNSSVisible = false
				form.validationVariables.isLocationAddressVisible = false
			
			endif
		
		else
			
			//rozsvitim jen UNLocode
			form.validationVariables.isLocationUNLocodeVisible = true
			form.validationVariables.isLocationGNSSVisible = false
			form.validationVariables.isLocationAddressVisible = false
		
		endif
		
	else
		if isTrue(params.data.qualifierOfIdentification.Code = "Z") then
		
			//zhasnu UNLocode a GNSS, ale pokud v nich mam nejaka data, tak se zeptam, jestli je muzu smazat
			if (params.data.UNLocode.hasValue() OR params.data.GNSS.hasValue()) then
			
				if system.confirm("This change can delete some data, do you want to continue?") then
				
					params.data.UNLocode.clear()
					params.data.GNSS.clear()
					
					//rozsvitim pouze adresu
					form.validationVariables.isLocationUNLocodeVisible = false
					form.validationVariables.isLocationGNSSVisible = false
					form.validationVariables.isLocationAddressVisible = true
					
				endif
			
			else
			
				//rozsvitim pouze adresu
				form.validationVariables.isLocationUNLocodeVisible = false
				form.validationVariables.isLocationGNSSVisible = false
				form.validationVariables.isLocationAddressVisible = true
			
			endif
			
		else
			//not known code
			
		endif
	endif
endif]]></Script>
      </ActionLangBody>
      <DataContext>
        <ActionDataContext />
      </DataContext>
    </ActionBlock>
