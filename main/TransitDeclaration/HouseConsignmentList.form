      <ActionLangBody IsValid="True">
        <Script><![CDATA[result = False
var mockGuid = "33107a15-395e-4205-849e-1b5bde10035f"

if form.GUID = mockGuid then
	apiobjects.transitDeclaration.fromMock() // navigation still relies on mock DATA!!!

	var data = apiobjects.transitDeclaration.getHouseConsignmentList()
	form.houseConsignmentRowList.fromJson(data)

else
	if form.GUID.hasValue() then
		result = form.actions.getDataFromCic()
	else
		result = true
	endIf
	//FIX ME - scenario - mode.edit + form.guid has OTHER value
