    <ActionBlock>
      <Name value="init" />
      <ActionLangBody IsValid="True">
        <Script><![CDATA[globals.busyIndicatorIsRunning = true

if form.GUID.hasValue() then
	
	if form.mode.isEmpty() then
	
		//TODO - what should happen if accept is empty
	
	else
		form.actions.loadCodelists()
		
		var requestJsonString = "{\"GUID\":\""+ form.GUID +"\"}"
		
		var response = apiobjects.CIC.postAsync("FBDataRequest", requestJsonString)
	
		if response.isError() then
			apiobjects.notificationsCtrl.addError(localized.FailedToLoadData)
			//Data load failed -> go to Declist
			navigation.main.DecList.go()
		else
		
			if form.isAccept then 
				form.modelAccept.fromJson(response.getJsonString())
				
				form.actions.setAuxiliaryVariables()
				
				form.modelAccept.setTouched(false)
			else
				form.modelNotAccept.fromJson(response.getJsonString())
				form.modelNotAccept.setTouched(false)
			endif
			
			
			
		endif
	
	endif
	
else
	apiobjects.notificationsCtrl.addError(localized.ThereIsNoDeclarationWithSuchAnID)
	//Without GUID there is no way how to load a declaration -> go to Declist
	navigation.main.DecList.go()

endif

globals.busyIndicatorIsRunning = false]]></Script>
      </ActionLangBody>
      <DataContext>
        <ActionDataContext />
      </DataContext>
    </ActionBlock>
    <ActionBlock>
      <Name value="handler_btn_accept_OnClick" />
      <IsPrivate value="True" />
      <ActionLangBody IsValid="True">
        <Script><![CDATA[globals.busyIndicatorIsRunning = true
form.modelAccept.setTouched(true)
var clientValidationErrorsCount = form.modelAccept.validationErrorsCount()

if clientValidationErrorsCount = 0 then
	
	var messageType = "FBAccept"
	var requestJsonString = form.modelAccept.toJson(true)
	
	//I need response -> I can't use global action send message
	form.auxiliaryVariables._isValidationSummaryVisible = form.actions.sendFBAccept()
	
	globals.busyIndicatorIsRunning = false
	
	if isFalse(form.auxiliaryVariables._isValidationSummaryVisible) then
		navigation.main.acceptConfirm.showDialog(form.response.TransitOperation.MRN)
		navigation.main.DecList.withInputNavTree("isAcceptedActive").go()
	endif
	
else

	form.auxiliaryVariables._isValidationSummaryVisible = true
	globals.busyIndicatorIsRunning = false
	
endif]]></Script>
      </ActionLangBody>
      <DataContext>
        <ActionDataContext />
      </DataContext>
    </ActionBlock>
