	endif
	
endif

if (form.modelAccept.Consignment.LocationOfGoods.UNLocode.hasValue() AND NOT isTrue(qualifierOfIdentification = "U")) then
	
	if system.confirm(localized.ThisWillDeleteData) then
		form.modelAccept.Consignment.LocationOfGoods.UNLocode.clear()
	else
		res = false
	endif
	
endif

if ((form.modelAccept.Consignment.LocationOfGoods.CustomsOffice.hasValue()) AND 
NOT isTrue(qualifierOfIdentification = "V")) then
	
	if system.confirm(localized.ThisWillDeleteData) then
		form.modelAccept.Consignment.LocationOfGoods.CustomsOffice.clear()
	else
		res = false
	endif
	
endif

if (form.modelAccept.Consignment.LocationOfGoods.GNSS.hasValue() AND NOT isTrue(qualifierOfIdentification = "W")) then
	
	if system.confirm(localized.ThisWillDeleteData) then
		form.modelAccept.Consignment.LocationOfGoods.GNSS.clear()
	else
		res = false
	endif
	
endif

if (form.modelAccept.Consignment.LocationOfGoods.EconomicOperator.hasValue() AND NOT isTrue(qualifierOfIdentification = "X")) then
	
	if system.confirm(localized.ThisWillDeleteData) then
		form.modelAccept.Consignment.LocationOfGoods.EconomicOperator.clear()
	else
		res = false
	endif
	
endif

if (form.modelAccept.Consignment.LocationOfGoods.Address.hasValue() AND NOT isTrue(qualifierOfIdentification = "Z")) then
	
	if system.confirm(localized.ThisWillDeleteData) then
		form.modelAccept.Consignment.LocationOfGoods.Address.clear()
	else
		res = false
	endif
	
endif

if isTrue(res) then
	form.modelAccept.Consignment.LocationOfGoods._QoI = form.modelAccept.Consignment.LocationOfGoods.qualifierOfIdentification.toJson()
else
	form.modelAccept.Consignment.LocationOfGoods.qualifierOfIdentification.fromJson(form.modelAccept.Consignment.LocationOfGoods._QoI)
endif]]></Script>
