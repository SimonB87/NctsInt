    <ActionBlock>
      <Name value="setLoGVisibility" />
      <ActionLangBody IsValid="True">
        <Script><![CDATA[//hide all
form.actions.clearLoGVisibility()

//if there is a value in qualifier of identification, show fields which are necessary
if form.modelAccept.Consignment.LocationOfGoods.qualifierOfIdentification.hasValue() then
	if isTrue(form.modelAccept.Consignment.LocationOfGoods.qualifierOfIdentification.Code = "Z") then
		form.auxiliaryVariables._isAddressVisible = true
	else
		if isTrue(form.modelAccept.Consignment.LocationOfGoods.qualifierOfIdentification.Code = "X") then
			form.auxiliaryVariables._isEconomicOperatorVisible = true
		else
			if isTrue(form.modelAccept.Consignment.LocationOfGoods.qualifierOfIdentification.Code = "Y") then
				form.auxiliaryVariables._isAuthorisationNumberVisible = true
			else
				if isTrue(form.modelAccept.Consignment.LocationOfGoods.qualifierOfIdentification.Code = "W") then
					form.auxiliaryVariables._isGNSSVisible = true
				else
					if isTrue(form.modelAccept.Consignment.LocationOfGoods.qualifierOfIdentification.Code = "V") then
						form.auxiliaryVariables._isCustomsOfficeVisible = true
						form.auxiliaryVariables._isContactPersonVisible = false
						form.modelAccept.Consignment.LocationOfGoods.ContactPerson.clear()
					else
						if isTrue(form.modelAccept.Consignment.LocationOfGoods.qualifierOfIdentification.Code = "U") then
							form.auxiliaryVariables._isUNLocodeVisible = true
						else
							if isTrue(form.modelAccept.Consignment.LocationOfGoods.qualifierOfIdentification.Code = "T") then
								form.auxiliaryVariables._isAddressTVisible = true							
							endif
						endif
					endif
				endif
			endif
		endif
	endif
endif]]></Script>
