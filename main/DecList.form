    <ActionBlock>
      <Name value="init" />
      <ActionLangBody IsValid="True">
        <Script><![CDATA[apiobjects.urlRouter.setDataHasChanged(false)
globals.DataChanged = false

apiobjects.urlRouter.clearUrl()

globals.globalUiState.userModeLabel = ""
globals.isUserInfoVisible = true
globals.isSettingsVisible = false
globals.Errors.clear()

apiobjects.RiskLevel.loadDataAsync(getDateTime())

if form.inputNavTree.isEmpty() then
	form.actions.setDeclist()
else
if isTrue(form.inputNavTree = "isClearanceInteruptedActive") then
	form.actions.clearInterruptOnClick()
else
	if isTrue(form.inputNavTree = "isAcceptedActive") then
		form.actions.acceptedSimplOnClick()
	else
		if isTrue(form.inputNavTree = "isNotReleasedActive") then
			form.actions.cancelledOnClick()
		else
			if isTrue(form.inputNavTree = "isSubmittedActive") then
				form.actions.submittedOnClick()
			else
				if isTrue(form.inputNavTree = "isWaitingForPresentationActive") then
					form.actions.waitingForPresentationOnClick()
				else
					if isTrue(form.inputNavTree = "isReleasedActive") then
						form.actions.releasedOnClick()
					else
						if isTrue(form.inputNavTree = "isFallbackActive") then
							form.actions.fallbackOnClick()
						else
							if isTrue(form.inputNavTree = "isWaitingForPresentationTRAActive") then
								form.actions.waitingForPresentationTRAonClick()
							else
								if isTrue(form.inputNavTree = "isPresentedAtCOoTraActive") then
									form.actions.presentedAtCOoTraOnClick()
								else
									if isTrue(form.inputNavTree = "isCrossingBorderActive") then
										form.actions.crossingBorderOnClick()
									else
										if isTrue(form.inputNavTree = "isMovementStoppedActive") then
											form.actions.movementStoppedOnClick()
										else
											if isTrue(form.inputNavTree = "isMovementTurnedBackActive") then
												form.actions.movementTurnedBackOnClick()
											else
												if isTrue(form.inputNavTree = "isWrittenOffActive") then
													form.actions.writtenOffOnClick()
												endif
											endif
										endif
									endif
								endif
							endif
						endif
					endif
				endif
			endif
		endif
	endif
endif
endif

form.apiobjects.listCtrl.setReady(true)
form.apiobjects.listCtrlDES.setReady(true)
form.apiobjects.listCtrlINC.setReady(true)
form.apiobjects.listCtrlTRA.setReady(true)
form.apiobjects.listCtrlEXIT.setReady(true)]]></Script>
      </ActionLangBody>
      <DataContext>
        <ActionDataContext />
      </DataContext>
    </ActionBlock>
    <ActionBlock>
      <Name value="goToDetailButton" />
      <ActionLangBody IsValid="True">
        <Script><![CDATA[apiobjects.transitDeclaration.clear()
if isTrue(globals.globalUiState.lastSelectedOffices = "customsOfficeOfDestination") then
	//save list state
	globals.globalUIState.listState = form.apiobjects.listCtrlDES.saveState(defaultString())
	globals.globalUiState.visibleColumns = form.apiobjects.listCtrlDES.getVisibleColumns()
	navigation.main.Header.withGuid(form.decListDESReferenceRow.GUID).go(enums.declarationFormMode.detail)
else
	if isTrue(globals.globalUiState.lastSelectedOffices = "customsOfficeOfDeparture") then
		//save list state
		globals.globalUIState.listState = form.apiobjects.listCtrl.saveState(defaultString())
		globals.globalUiState.visibleColumns = form.apiobjects.listCtrl.getVisibleColumns()
		if isTrue(form.decListReferenceRow.State = "Dep_Interrupted") then
			navigation.main.Header.withGuid(form.decListReferenceRow.GUID).go(enums.declarationFormMode.incompleteDetail)
		else
			navigation.main.Header.withGuid(form.decListReferenceRow.GUID).go(enums.declarationFormMode.detail)
		endif
	else
		if isTrue(globals.globalUiState.lastSelectedOffices = "customsOfficeOfIncidentRegistration") then
			//save list state
			globals.globalUIState.listState = form.apiobjects.listCtrlINC.saveState(defaultString())
			globals.globalUiState.visibleColumns = form.apiobjects.listCtrlINC.getVisibleColumns()
			navigation.main.Header.withGuid(form.decListINCReferenceRow.GUID).go(enums.declarationFormMode.detail)
		else
			if isTrue(globals.globalUiState.lastSelectedOffices = "customsOfficeOfTransit") then
				//save list state
				globals.globalUIState.listState = form.apiobjects.listCtrlTRA.saveState(defaultString())
				globals.globalUiState.visibleColumns = form.apiobjects.listCtrlTRA.getVisibleColumns()
				navigation.main.Header.withGuid(form.decListTRAReferenceRow.GUID).go(enums.declarationFormMode.detail)
			else
				notificationsCtrl.addError(localized.CustomsOfficeHasToBeChosen)
			endif
		endif
	endif
endif]]></Script>
