    <ActionBlock>
      <Name value="handler_button_registrationReturnedTAD_OnClick" />
      <IsPrivate value="True" />
      <ActionLangBody IsValid="True">
        <Script><![CDATA[if form.decListDESReferenceRow.MRN.hasValue() then
	
	if system.confirm("Do you want to register documents?") then
	
		form.RegistrationReturnedTAD.TransitOperation.MRN = form.decListDESReferenceRow.MRN
		var requestJSON = form.RegistrationReturnedTAD.toJson()
		var response = apiobjects.CIC.postAsync("FBDocumentsRegistration", requestJSON)
		
		if response.isError() then
			apiobjects.notificationsCtrl.addError("Server responded with an error.")
		endif
		
	endif
	
else
	apiobjects.notificationsCtrl.addError("MRN is missing.")
endif]]></Script>
      </ActionLangBody>
      <DataContext>
        <ActionDataContext />
      </DataContext>
