              <Validators>
                <Validator Name="CLEXCISE">
                  <MtsImportOptions.MTS.ID value="CLEXCISE" />
                  <MtsImportOptions.MTS.Type value="rule-constraint" />
                  <ErrorMessage IsValid="True">
                    <ExpressionText><![CDATA["CLEXCISE"]]></ExpressionText>
                    <ResultType>
                      <BaseType value="String" />
